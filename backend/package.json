{
  "name": "factory-os-backend",
  "version": "1.0.0",
  "description": "Backend API for Factory OS Core",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seedData.js",
    "seed:new": "node scripts/insertSyntheticData.js",
    "seed:scenarios": "node scripts/generateDataScenarios.js",
    "seed:generator": "node scripts/dataGenerator.js",
    "seed:normal": "node scripts/dataGenerator.js normal --clear",
    "seed:high-performance": "node scripts/dataGenerator.js highPerformance --clear",
    "seed:struggling": "node scripts/dataGenerator.js struggling --clear",
    "seed:startup": "node scripts/dataGenerator.js startup --clear"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
